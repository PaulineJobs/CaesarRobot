title __TEST's Class Diagram__\n

  namespace test {
    enum AxeRobot {
      X
      Y
    }
  }
  

  namespace test {
    class test.CommunicationRobot {
        + CommunicationRobot()
        {static} + main()
        ~ getInfoLidar()
        ~ lancerCalcul()
        ~ lancerCommunication()
        ~ resetRoues()
        ~ setEtatRobot()
        ~ setNombreTours()
    }
  }
  

