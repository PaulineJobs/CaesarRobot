        {static} + main()
        ~ calculPositionLidarX()
        ~ calculPositionLidarY()
        ~ setPositionObstacle()
    }
  }
  

  namespace test {
    class test.RobotPosition {
        ~ diametreRoue : double
        ~ nbrToursDroite : int
        ~ nbrToursGauche : int
        ~ pi : double
        ~ positionX : float
