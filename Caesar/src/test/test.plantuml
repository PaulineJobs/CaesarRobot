        ~ tourneAgauche()
    }
  }
  

  namespace test {
    class test.RobotPosition.RobotPositionHolder {
        {static} - instance : RobotPosition
    }
  }
  

  namespace test {
    enum SensAxe {
      NEGATIF
      POSITIF
    }
  }
  

  namespace test {
    class test.TraitementDonnees {
        + TraitementDonnees()
        {static} + main()
        + run()
    }
  }
  

  test.CommunicationRobot o-right- test.ObstaclePosition : obstaclePosition
  test.ObstaclePosition o-- test.RobotPosition : robotPosition
  test.RobotPosition o-right- test.AxeRobot : axeRobot
  test.RobotPosition o-left- test.EtatRobot : etatRobot
  test.RobotPosition o-- test.SensAxe : sensAxe
  test.RobotPosition +-down- test.RobotPosition.RobotPositionHolder
  test.TraitementDonnees o-right- test.CommunicationRobot : communicationRobot


